{
	"_version": "1.7.0",
	"sap.app": {
		"id": "z.anlt.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": []
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/SAP/ZDATA_SET_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": [
						"annotation0"
					]
				}
			},
			"annotation0": {
				"type": "ODataAnnotation",
				"uri": "localService/annotation.xml",
				"settings": {
					"localUri": "localService/annotation.xml"
				}
			}
		},
		"offline": false
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"resources": {
			"js": [],
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.collaboration": {
					"lazy": false
				},
				"sap.ushell": {
					"lazy": false
				},
				"sap.uxap": {
					"lazy": false
				},
				"sap.ui.core": {
					"lazy": false
				},
				"sap.m": {
					"lazy": false
				},
				"sap.f": {
					"lazy": false
				},
				"sap.ui.generic.app": {
					"lazy": false
				},
				"sap.suite.ui.generic.template": {
					"lazy": false
				},
				"sap.ui.comp": {
					"lazy": false
				},
				"sap.ui.layout": {
					"lazy": false
				},
				"sap.ui.table": {
					"lazy": false
				},
				"sap.ui.fl": {
					"lazy": false
				}	
			},
			"components": {}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"useBatch": true,
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"refreshAfterChange": false
				}
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.viewExtensions": {
					"sap.suite.ui.generic.template.AnalyticalListPage.view.AnalyticalListPage": {
						
					}
				},
				"sap.ui.controllerExtensions": {
					"sap.suite.ui.generic.template.AnalyticalListPage.view.AnalyticalListPage": {
						"controllerName": "z.anlt.app.ext.controller.AnalyticalListPageExt"
					}
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.ui.generic.app": {
		"_version": "1.2.0",
		"settings": {
			"flexibleColumnLayout":{  
				"defaultTwoColumnLayoutType":"TwoColumnsBeginExpanded"
			}
		},
		"pages": [{
			"entitySet": "ActDataSet",
			"component": {
				"name": "sap.suite.ui.generic.template.AnalyticalListPage",
				"list": true,
				"settings": {
					"tableSettings":{
						"type": "GridTable",
						"multiSelect": true,
						"selectAll": true,
						"selectionLimit": 20
					},
					"qualifier": "Default",
					"condensedTableLayout": true,
					"smartVariantManagement": true,
					"defaultContentView": "charttable",
					"defaultFilterMode": "visual",
					"showGoButtonOnFilterBar": true,
					"contentTitle": "{{contentAreaTitle}}",
					"autoHide":false,
					"filterDefaultsFromSelectionVariant":false
				}
			}
		}]
	}
}